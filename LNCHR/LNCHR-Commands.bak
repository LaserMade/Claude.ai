lngui_run_commands(input){if ((input == "/todo") and (UsingAnyWorkcomputer)) ; View Tasks (Outlook) {close_lngui()tryrun('"C:\Program Files (x86)\Microsoft Office\root\Office16\OUTLOOK.EXE" /recycle /select outlook:tasks')return}if ((input == "/wcal") and (UsingAnyWorkcomputer)) ; Work Calendar {close_lngui()tryrun('"C:\Program Files (x86)\Microsoft Office\root\Office16\OUTLOOK.EXE" /recycle /select outlook:calendar')return}if ((input == "/com") and (UsingAnyWorkcomputer)) ; Compose {close_lngui()outlookApp := GetOutlookCom()
MailItem := outlookApp.CreateItem(0)
MailItem.Displayreturn}if ((input == "/inb") and (UsingAnyWorkcomputer)) ; Work inbox {close_lngui()tryrun('"C:\Program Files (x86)\Microsoft Office\root\Office16\OUTLOOK.EXE" /recycle /select outlook:inbox')return}}